      "accepted_variants": ["nano notes/todo.txt"],
      "tags": ["editing", "basics"]
    },
    {
      "id": "ssh",
      "command": "ssh analyst@lab-vmx",
      "description": "Connect to another machine over SSH.",
      "mock_output": "analyst@lab-vmx's password: \n",
      "accepted_variants": ["ssh analyst@lab-vmx"],
      "tags": ["network", "remote"]
    },
    {
      "id": "scp",
      "command": "scp README.md analyst@lab-vmx:/tmp/README.md",
      "description": "Copy a file to a remote host for review.",
      "mock_output": "README.md                                   100%  512   512.0KB/s   00:00\n",
      "accepted_variants": ["scp README.md analyst@lab-vmx:/tmp/README.md"],
      "tags": ["network", "files"]
    },
    {
      "id": "curl",
      "command": "curl -I https://example.com",
      "description": "Check HTTP headers quickly.",
      "mock_output": "HTTP/2 200 \nserver: nginx\ncontent-type: text/html\n",
      "accepted_variants": ["curl -I https://example.com"],
      "tags": ["network", "basics"]
    },
    {
      "id": "ping",
      "command": "ping -c 1 1.1.1.1",
      "description": "Verify basic network reachability with one ping.",
      "mock_output": "1 packets transmitted, 1 received, 0% packet loss\n",
      "accepted_variants": ["ping -c 1 1.1.1.1"],
      "tags": ["network", "monitoring"]
