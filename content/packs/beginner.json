      "command": "touch notes.txt",
      "description": "Create or update the timestamp on a note file.",
      "mock_output": "",
      "accepted_variants": ["touch notes.txt"],
      "tags": ["files", "basics"]
    },
    {
      "id": "mkdir",
      "command": "mkdir logs",
      "description": "Make a new directory for files you collect.",
      "mock_output": "",
      "accepted_variants": ["mkdir logs"],
      "tags": ["files", "basics"]
    },
    {
      "id": "cp",
      "command": "cp README.md README.backup",
      "description": "Copy a file before editing.",
      "mock_output": "",
      "accepted_variants": ["cp README.md README.backup"],
      "tags": ["files", "safety"]
    },
    {
      "id": "mv",
      "command": "mv README.backup docs/readme.bak",
      "description": "Move or rename a file to a new location.",
      "mock_output": "",
      "accepted_variants": ["mv README.backup docs/readme.bak"],
      "tags": ["files", "basics"]
    },
    {
      "id": "cat",
      "command": "cat README.md",
      "description": "Display the contents of a file.",
      "mock_output": "HackType — type fast, stay sharp.\n",
      "accepted_variants": ["cat README.md"],
      "tags": ["files", "basics"]
    },
    {
      "id": "head",
      "command": "head -n 5 README.md",
      "description": "Preview the first lines of a file.",
      "mock_output": "HackType — type fast, stay sharp.\nIntroductie\n...\n",
      "accepted_variants": ["head -n 5 README.md"],
      "tags": ["files", "analysis"]
    },
    {
      "id": "tail",
      "command": "tail -n 5 README.md",
      "description": "Peek at the last lines for recent notes.",
      "mock_output": "Credits\nLicense: MIT\n",
      "accepted_variants": ["tail -n 5 README.md"],
      "tags": ["files", "analysis"]
    },
    {
      "id": "wc",
      "command": "wc -l README.md",
      "description": "Count lines to estimate file length quickly.",
      "mock_output": "42 README.md\n",
      "accepted_variants": ["wc -l README.md"],
      "tags": ["files", "analysis"]
    },
    {
      "id": "grep",
      "command": "grep HackType README.md",
      "description": "Search for a keyword inside a file.",
      "mock_output": "HackType — type fast, stay sharp.\n",
      "accepted_variants": ["grep HackType README.md"],
      "tags": ["search", "analysis"]
    },
    {
      "id": "find",
      "command": "find . -maxdepth 1",
      "description": "List files in the current folder without recursing deeply.",
      "mock_output": "./\n./README.md\n./notes\n./scripts\n",
      "accepted_variants": ["find . -maxdepth 1"],
      "tags": ["search", "navigation"]
    },
    {
      "id": "locate",
      "command": "locate bashrc",
